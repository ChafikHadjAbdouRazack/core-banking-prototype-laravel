{
  "description": "FinAegis Treasury Domain - Portfolio management and yield optimization",
  "editable": true,
  "id": null,
  "panels": [
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "currencyUSD"}},
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "id": 1,
      "title": "Total Assets Under Management",
      "type": "stat",
      "targets": [{"expr": "sum(treasury_portfolio_value_total)", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "percentunit", "thresholds": {"steps": [{"color": "red", "value": null}, {"color": "yellow", "value": 0.03}, {"color": "green", "value": 0.05}]}}},
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "id": 2,
      "title": "Portfolio Yield (APY)",
      "type": "stat",
      "targets": [{"expr": "treasury_portfolio_yield_apy", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "short"}},
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "id": 3,
      "title": "Active Portfolios",
      "type": "stat",
      "targets": [{"expr": "treasury_active_portfolios_count", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "percentunit", "thresholds": {"steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 0.001}, {"color": "red", "value": 0.01}]}}},
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "id": 4,
      "title": "NAV Calculation Deviation",
      "type": "stat",
      "targets": [{"expr": "abs(treasury_nav_deviation)", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "id": 5,
      "title": "Portfolio Value Over Time",
      "type": "timeseries",
      "targets": [{"expr": "sum(treasury_portfolio_value_total) by (portfolio)", "legendFormat": "{{portfolio}}", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "id": 6,
      "title": "Cash Allocation by Currency",
      "type": "piechart",
      "targets": [{"expr": "treasury_cash_allocation_total", "legendFormat": "{{currency}}", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "id": 7,
      "title": "Yield Optimization Operations",
      "type": "timeseries",
      "targets": [
        {"expr": "rate(treasury_optimization_runs_total[5m])", "legendFormat": "Optimizations", "refId": "A"},
        {"expr": "rate(treasury_rebalancing_operations_total[5m])", "legendFormat": "Rebalancing", "refId": "B"}
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "ms"}},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "id": 8,
      "title": "NAV Calculation Time",
      "type": "timeseries",
      "targets": [
        {"expr": "histogram_quantile(0.50, rate(treasury_nav_calculation_seconds_bucket[5m])) * 1000", "legendFormat": "p50", "refId": "A"},
        {"expr": "histogram_quantile(0.99, rate(treasury_nav_calculation_seconds_bucket[5m])) * 1000", "legendFormat": "p99", "refId": "B"}
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["finaegis", "treasury", "portfolio"],
  "time": {"from": "now-1h", "to": "now"},
  "title": "FinAegis Treasury Domain",
  "uid": "finaegis-treasury",
  "version": 1
}
