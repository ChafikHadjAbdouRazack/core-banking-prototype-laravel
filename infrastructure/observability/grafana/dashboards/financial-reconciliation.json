{
  "description": "FinAegis Financial Reconciliation - Transaction reconciliation and audit metrics",
  "editable": true,
  "id": null,
  "panels": [
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "short"}},
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "id": 1,
      "title": "Transactions Reconciled (24h)",
      "type": "stat",
      "targets": [{"expr": "increase(reconciliation_transactions_processed_total[24h])", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "short", "thresholds": {"steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 1}, {"color": "red", "value": 10}]}}},
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "id": 2,
      "title": "Unmatched Items",
      "type": "stat",
      "targets": [{"expr": "reconciliation_unmatched_items_total", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "currencyUSD", "thresholds": {"steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 100}, {"color": "red", "value": 1000}]}}},
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "id": 3,
      "title": "Discrepancy Amount",
      "type": "stat",
      "targets": [{"expr": "reconciliation_discrepancy_amount_total", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "percentunit", "thresholds": {"steps": [{"color": "red", "value": null}, {"color": "yellow", "value": 0.95}, {"color": "green", "value": 0.99}]}}},
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "id": 4,
      "title": "Match Rate",
      "type": "stat",
      "targets": [{"expr": "reconciliation_match_rate", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "id": 5,
      "title": "Reconciliation Status",
      "type": "timeseries",
      "targets": [
        {"expr": "rate(reconciliation_transactions_processed_total{status=\"matched\"}[5m])", "legendFormat": "Matched", "refId": "A"},
        {"expr": "rate(reconciliation_transactions_processed_total{status=\"unmatched\"}[5m])", "legendFormat": "Unmatched", "refId": "B"},
        {"expr": "rate(reconciliation_transactions_processed_total{status=\"exception\"}[5m])", "legendFormat": "Exception", "refId": "C"}
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "id": 6,
      "title": "Discrepancies by Type",
      "type": "piechart",
      "targets": [{"expr": "reconciliation_discrepancies_by_type_total", "legendFormat": "{{type}}", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "s"}},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "id": 7,
      "title": "Reconciliation Latency",
      "type": "timeseries",
      "targets": [
        {"expr": "histogram_quantile(0.50, rate(reconciliation_duration_seconds_bucket[5m]))", "legendFormat": "p50", "refId": "A"},
        {"expr": "histogram_quantile(0.95, rate(reconciliation_duration_seconds_bucket[5m]))", "legendFormat": "p95", "refId": "B"}
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "id": 8,
      "title": "Audit Trail Events",
      "type": "timeseries",
      "targets": [{"expr": "rate(audit_trail_events_total[5m])", "legendFormat": "{{event_type}}", "refId": "A"}]
    }
  ],
  "refresh": "1m",
  "schemaVersion": 38,
  "tags": ["finaegis", "reconciliation", "audit"],
  "time": {"from": "now-6h", "to": "now"},
  "title": "FinAegis Financial Reconciliation",
  "uid": "finaegis-reconciliation",
  "version": 1
}
