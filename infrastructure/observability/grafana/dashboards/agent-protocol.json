{
  "description": "FinAegis Agent Protocol - AI agent payment and activity metrics",
  "editable": true,
  "id": null,
  "panels": [
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "short"}},
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "id": 1,
      "title": "Active Agents",
      "type": "stat",
      "targets": [{"expr": "agent_protocol_active_agents_total", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "short"}},
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "id": 2,
      "title": "Transactions (24h)",
      "type": "stat",
      "targets": [{"expr": "increase(agent_protocol_transactions_total[24h])", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "currencyUSD"}},
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "id": 3,
      "title": "Transaction Volume (24h)",
      "type": "stat",
      "targets": [{"expr": "increase(agent_protocol_transaction_volume_total[24h])", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "short"}},
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "id": 4,
      "title": "Active Escrows",
      "type": "stat",
      "targets": [{"expr": "agent_protocol_active_escrows_total", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "id": 5,
      "title": "Agent Transaction Rate",
      "type": "timeseries",
      "targets": [{"expr": "rate(agent_protocol_transactions_total[5m])", "legendFormat": "{{status}}", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "id": 6,
      "title": "Reputation Score Distribution",
      "type": "histogram",
      "targets": [{"expr": "agent_protocol_reputation_score_bucket", "legendFormat": "{{le}}", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "id": 7,
      "title": "MCP Tool Executions",
      "type": "timeseries",
      "targets": [{"expr": "rate(agent_mcp_tool_executions_total[5m])", "legendFormat": "{{tool}}", "refId": "A"}]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {"defaults": {"unit": "ms"}},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "id": 8,
      "title": "Transaction Processing Latency",
      "type": "timeseries",
      "targets": [
        {"expr": "histogram_quantile(0.50, rate(agent_protocol_transaction_duration_seconds_bucket[5m])) * 1000", "legendFormat": "p50", "refId": "A"},
        {"expr": "histogram_quantile(0.99, rate(agent_protocol_transaction_duration_seconds_bucket[5m])) * 1000", "legendFormat": "p99", "refId": "B"}
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["finaegis", "agent-protocol", "ai"],
  "time": {"from": "now-1h", "to": "now"},
  "title": "FinAegis Agent Protocol",
  "uid": "finaegis-agent-protocol",
  "version": 1
}
