{
    "$schema": "https://finaegis.io/schemas/module.json",
    "name": "finaegis/compliance",
    "version": "1.3.0",
    "description": "KYC/AML compliance, risk assessment, and regulatory monitoring",
    "type": "core",
    "dependencies": {
        "required": {
            "finaegis/shared": "^1.0",
            "finaegis/account": "^1.0",
            "finaegis/user": "^1.0"
        },
        "optional": {}
    },
    "provides": {
        "interfaces": [
            "App\\Domain\\Compliance\\Contracts\\KycServiceInterface",
            "App\\Domain\\Compliance\\Contracts\\AmlServiceInterface",
            "App\\Domain\\Compliance\\Contracts\\RiskAssessmentInterface"
        ],
        "events": [
            "KycVerificationStarted",
            "KycVerificationCompleted",
            "KycVerificationFailed",
            "AmlAlertRaised",
            "RiskScoreUpdated",
            "ComplianceAlertCreated"
        ],
        "commands": [
            "InitiateKycCommand",
            "UpdateKycStatusCommand",
            "PerformAmlCheckCommand"
        ]
    },
    "paths": {
        "routes": "Routes/api.php",
        "migrations": "Database/Migrations",
        "config": "Config/compliance.php",
        "tests": "Tests"
    }
}
