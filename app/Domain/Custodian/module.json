{
    "$schema": "https://finaegis.io/schemas/module.json",
    "name": "finaegis/custodian",
    "version": "1.3.0",
    "description": "Bank integration, custodian services, and webhook processing",
    "type": "optional",
    "dependencies": {
        "required": {
            "finaegis/shared": "^1.0",
            "finaegis/account": "^1.0"
        },
        "optional": {
            "finaegis/banking": "^1.0",
            "finaegis/webhook": "^1.0"
        }
    },
    "provides": {
        "interfaces": [
            "App\\Domain\\Custodian\\Contracts\\CustodianServiceInterface"
        ],
        "events": [
            "CustodianAccountCreated",
            "CustodianTransferInitiated",
            "CustodianTransferCompleted",
            "WebhookReceived"
        ],
        "commands": [
            "CreateCustodianAccountCommand",
            "InitiateCustodianTransferCommand"
        ]
    },
    "paths": {
        "routes": "Routes/api.php",
        "migrations": "Database/Migrations",
        "tests": "Tests"
    }
}
