{
    "$schema": "https://finaegis.io/schemas/module.json",
    "name": "finaegis/webhook",
    "version": "1.3.0",
    "description": "Webhook management and delivery with retry handling",
    "type": "optional",
    "dependencies": {
        "required": {
            "finaegis/shared": "^1.0"
        },
        "optional": {}
    },
    "provides": {
        "interfaces": [
            "App\\Domain\\Webhook\\Contracts\\WebhookServiceInterface"
        ],
        "events": [
            "WebhookRegistered",
            "WebhookDelivered",
            "WebhookFailed",
            "WebhookRetried"
        ],
        "commands": [
            "RegisterWebhookCommand",
            "DeliverWebhookCommand",
            "RetryWebhookCommand"
        ]
    },
    "paths": {
        "routes": "Routes/api.php",
        "migrations": "Database/Migrations",
        "tests": "Tests"
    }
}
