{
    "$schema": "https://finaegis.io/schemas/module.json",
    "name": "finaegis/performance",
    "version": "1.3.0",
    "description": "Performance tracking, profiling, and optimization",
    "type": "optional",
    "dependencies": {
        "required": {
            "finaegis/shared": "^1.0"
        },
        "optional": {
            "finaegis/monitoring": "^1.0"
        }
    },
    "provides": {
        "interfaces": [],
        "events": [
            "PerformanceReportGenerated",
            "BottleneckDetected"
        ],
        "commands": [
            "GeneratePerformanceReportCommand",
            "RunBenchmarkCommand"
        ]
    },
    "paths": {
        "migrations": "Database/Migrations",
        "tests": "Tests"
    }
}
